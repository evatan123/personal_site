library(dplyr)
library(readr)
library(stats)
library(knitr)
library(ggplot2)
library(tidyr)
gapminder=read.csv("http://bioconnector.org/data/gapminder.csv")
colnames(gapminder)=c("Country","Continent","Year","Life.Expectancy","Population","GDP.per.Capita")
table.1=gapminder %>%
group_by(Continent) %>%
summarise(Country=n_distinct(Country))
kable(table.1,col.names=c("Continent","Country"),align="c")
ggplot(data=table.1)+
geom_col(mapping=aes(Continent,Country))+
ggtitle("Country Number of Each Continent")+
xlab("Continent")+
ylab("Country")+
theme(plot.title=element_text(size=20,hjust=0.5))
le=tapply(gapminder$Life.Expectancy,list(gapminder$Continent,gapminder$Year),mean)
le=data.frame(t(le))
Year=seq(from=1952,to=2007,by=5)
le=cbind(Year,le)
row.names(le)=1:12
p=ggplot(data=le,aes())+
geom_point(mapping=aes(Year,Africa),color=1)+
geom_line(mapping=aes(Year,Africa),color=1,linetype="dashed")+
geom_point(mapping=aes(Year,Americas),color=2)+
geom_line(mapping=aes(Year,Americas),color=2,linetype="dashed")+
geom_point(mapping=aes(Year,Asia),color=3)+
geom_line(mapping=aes(Year,Asia),color=3,linetype="dashed")+
geom_point(mapping=aes(Year,Europe),color=4)+
geom_line(mapping=aes(Year,Europe),color=4,linetype="dashed")+
geom_point(mapping=aes(Year,Oceania),color=5)+
geom_line(mapping=aes(Year,Oceania),color=5,linetype="dashed")+
ggtitle("Average Life Expectancy")+
xlab("Year")+
ylab("Life Expectancy (Years)")+
theme(plot.title=element_text(size=20,hjust=0.5))+
scale_color_manual("",breaks = c("Africa", "Americas", "Asia", "Europe", "Oceania"),values = c(1,2,3,4,5))
p
le_1 <- gather(le, Continent, Average.Life.Expectancy, -Year)
le_1
ggplot(data = le_1, aes(x = Year, y= Average.Life.Expectancy, color = Continent)) +
geom_point()+
geom_line()
